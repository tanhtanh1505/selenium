{
  "url": "http://localhost:3000",
  "addItem": {
    "description": "Add an item to the list",
    "testCases": [
      {
        "description": "Add normal item to the list",
        "input": "Buy groceries",
        "expected": "Buy groceries"
      },
      {
        "description": "Add special characters to the list",
        "input": "!@#$%^&*()_",
        "expected": "!@#$%^&*()_"
      },
      {
        "description": "Add sentence with numbers to the list",
        "input": "123123123",
        "expected": "123123123"
      },
      {
        "description": "Add full space characters to the list",
        "input": "   ",
        "expected": null
      },
      {
        "description": "Add empty string to the list",
        "input": "",
        "expected": null
      }
    ]
  }
}
